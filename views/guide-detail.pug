// Sign Up Window Code
#signin.modal.fade(tabindex='-1' role='dialog' aria-labelledby='myModalLabel1' aria-hidden='true')
  .modal-dialog
    #myModalLabel1.modal-content
      .modal-body
        .text-center
          img.img-responsive(src='assets/img/logo.png' alt='')
        // Nav tabs
        ul.nav.nav-tabs.nav-advance.theme-bg(role='tablist')
          li.nav-item.active
            a.nav-link(data-toggle='tab' href='#employer' role='tab')
              i.ti-user
              |  Employer
          li.nav-item
            a.nav-link(data-toggle='tab' href='#candidate' role='tab')
              i.ti-user
              |  Candidate
        // Nav tabs
        // Tab panels
        .tab-content
          // Employer Panel 1
          #employer.tab-pane.fade.in.show.active(role='tabpanel')
            form
              .form-group
                label User Name
                input.form-control(type='text' placeholder='User Name')
              .form-group
                label Password
                input.form-control(type='password' placeholder='*********')
              .form-group
                span.custom-checkbox
                  input#4(type='checkbox')
                  label(for='4')
                  | Remember me
                a.fl-right(href='#' title='Forget') Forgot Password?
              .form-group.text-center
                button.btn.theme-btn.full-width.btn-m(type='button') LogIn 
            .log-option
              span OR
            .row.mrg-bot-20
              .col-md-6
                a.fb-log-btn.log-btn(href='#' title='')
                  i.fa.fa-facebook
                  | Sign In With Facebook
              .col-md-6
                a.gplus-log-btn.log-btn(href='#' title='')
                  i.fa.fa-google-plus
                  | Sign In With Google+
          // /.Panel 1
          // Candidate Panel 2
          #candidate.tab-pane.fade(role='tabpanel')
            form
              .form-group
                label User Name
                input.form-control(type='text' placeholder='User Name')
              .form-group
                label Password
                input.form-control(type='password' placeholder='*********')
              .form-group
                span.custom-checkbox
                  input#4(type='checkbox')
                  label(for='4')
                  | Remember me
                a.fl-right(href='#' title='Forget') Forgot Password?
              .form-group.text-center
                button.btn.theme-btn.full-width.btn-m(type='button') LogIn 
            .log-option
              span OR
            .row.mrg-bot-20
              .col-md-6
                a.fb-log-btn.log-btn(href='#' title='')
                  i.fa.fa-facebook
                  | Sign In With Facebook
              .col-md-6
                a.gplus-log-btn.log-btn(href='#' title='')
                  i.fa.fa-google-plus
                  | Sign In With Google+
          // /.Panel 2
        // Tab panels
// End Sign Up Window
// Switcher
button.w3-button.w3-teal.w3-xlarge.w3-right(onclick='openRightMenu()')
  i.spin.theme-cl.fa.fa-cog(aria-hidden='true')
#rightMenu.w3-sidebar.w3-bar-block.w3-card-2.w3-animate-right(style='display:none;right:0;')
  button.w3-bar-item.w3-button.w3-large.theme-bg(onclick='closeRightMenu()') Close &times;
  ul#styleOptions(title='switch styling')
    li
      a.cl-box.cl-default(href='javascript: void(0)' data-theme='skins/default')
    li
      a.cl-box.cl-red(href='javascript: void(0)' data-theme='skins/red')
    li
      a.cl-box.cl-green(href='javascript: void(0)' data-theme='skins/green')
    li
      a.cl-box.cl-blue(href='javascript: void(0)' data-theme='skins/blue')
    li
      a.cl-box.cl-pink(href='javascript: void(0)' data-theme='skins/pink')
    li
      a.cl-box.cl-purple(href='javascript: void(0)' data-theme='skins/purple')
