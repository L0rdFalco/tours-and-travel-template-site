// Dashboard Js
script(src='assets/plugins/js/jquery.slimscroll.min.js')
script(src='assets/plugins/js/jquery.metisMenu.js')
script(src='assets/plugins/js/jquery.easing.min.js')
script(type='text/javascript').
  $('.countdown').downCount({
  date: '09/10/2019 12:00:00',
  offset: +10
  }, function () {
  alert('WOOT WOOT, done!');
  });
script(src='assets/js/jQuery.style.switcher.js')
script.
  function openRightMenu() {
  document.getElementById("rightMenu").style.display = "block";
  }
  function closeRightMenu() {
  document.getElementById("rightMenu").style.display = "none";
  }
script(type='text/javascript').
  $(document).ready(function() {
  $('#styleOptions').styleSwitcher();
  });
